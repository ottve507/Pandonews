<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Location</th>
    <th>Language</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed.title %></td>
    <td><%= feed.content %></td>
    <td><%= feed.location %></td>
    <td><%= feed.language %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>

  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed', new_feed_path %>
